<!DOCTYPE data-dictionary SYSTEM "data-dictionary.dtd">
<!-- $Id: data-dictionary.xml,v 1.22 2011/06/29 06:20:06 vinod Exp $ -->
<data-dictionary name="ShoppingCart">
    <table name="CartUser">
        <columns>
            <column name="USER_ID">
                <data-type>INTEGER</data-type>
                <uniquevalue-generation>
                    <generator-name>CartUser.USER_ID</generator-name>
                </uniquevalue-generation>
            </column>
            <column name="USERNAME">
                <data-type>CHAR</data-type>
                <max-size>50</max-size>
                <nullable>false</nullable>
            </column>
            <column name="PASSWORD">
                <data-type>CHAR</data-type>
                <max-size>50</max-size>
                <nullable>false</nullable>
            </column>
            <column name="PHONE_NUMBER">
                <data-type>BIGINT</data-type>
                <max-size>20</max-size>
                <nullable>false</nullable>
            </column>
            <column name="IS_PRIVILEGE">
                <data-type>BOOLEAN</data-type>
                <default-value>false</default-value>
                <nullable>false</nullable>
            </column>
        </columns>
        <primary-key name="USER_PK">
            <primary-key-column>USER_ID</primary-key-column>
        </primary-key>
    </table>
    <table name="Product">
        <columns>
            <column name="PRODUCT_ID">
                <data-type>INTEGER</data-type>
                <uniquevalue-generation>
                    <generator-name>Product.PRODUCT_ID</generator-name>
                </uniquevalue-generation>
            </column>
            <column name="PRODUCT_NAME">
                <data-type>CHAR</data-type>
                <max-size>50</max-size>
                <nullable>false</nullable>
            </column>
            <column name="PRODUCT_SPEC">
                <data-type>CHAR</data-type>
                <max-size>100</max-size>
                <nullable>false</nullable>
            </column>
            <column name="PRODUCT_PRICE">
                <data-type>INTEGER</data-type>
                <max-size>10</max-size>
                <nullable>false</nullable>
            </column>
            <column name="PRODUCT_QUANTITY">
                <data-type>INTEGER</data-type>
                <max-size>10</max-size>
                <nullable>false</nullable>
            </column>
            <column name="IS_AVAILABLE">
                <data-type>BOOLEAN</data-type>
                <default-value>true</default-value>
                <nullable>false</nullable>
            </column>
        </columns>
        <primary-key name="PRODUCT_PK">
            <primary-key-column>PRODUCT_ID</primary-key-column>
        </primary-key>
    </table>
    <table name="UserWishList">
        <columns>
            <column name="PRODUCT_ID">
                <data-type>INTEGER</data-type>
            </column>
            <column name="USER_ID">
                <data-type>INTEGER</data-type>
            </column>
        </columns>
        <primary-key name="WISHLIST_PK">
            <primary-key-column>USER_ID</primary-key-column>
            <primary-key-column>PRODUCT_ID</primary-key-column>
        </primary-key>
        <foreign-keys>
            <foreign-key name="WISHLIST_FK1" reference-table-name="CartUser">
                <fk-columns>
                    <fk-column>
                        <fk-local-column>USER_ID</fk-local-column>
                        <fk-reference-column>USER_ID</fk-reference-column>
                    </fk-column>
                </fk-columns>
            </foreign-key>
            <foreign-key name="WISHLIST_FK2" reference-table-name="Product">
                <fk-columns>
                    <fk-column>
                        <fk-local-column>PRODUCT_ID</fk-local-column>
                        <fk-reference-column>PRODUCT_ID</fk-reference-column>
                    </fk-column>
                </fk-columns>
            </foreign-key>
        </foreign-keys>
    </table>
    <table name="Cart">
        <columns>
            <column name="PRODUCT_ID">
                <data-type>INTEGER</data-type>
            </column>
            <column name="USER_ID">
                <data-type>INTEGER</data-type>
            </column>
            <column name="QUANTITY">
                <data-type>INTEGER</data-type>
                <nullable>false</nullable>
            </column>
        </columns>
        <primary-key name="CART_PK">
            <primary-key-column>USER_ID</primary-key-column>
            <primary-key-column>PRODUCT_ID</primary-key-column>
        </primary-key>
        <foreign-keys>
            <foreign-key name="CART_FK1" reference-table-name="CartUser">
                <fk-columns>
                    <fk-column>
                        <fk-local-column>USER_ID</fk-local-column>
                        <fk-reference-column>USER_ID</fk-reference-column>
                    </fk-column>
                </fk-columns>
            </foreign-key>
            <foreign-key name="CART_FK2" reference-table-name="Product">
                <fk-columns>
                    <fk-column>
                        <fk-local-column>PRODUCT_ID</fk-local-column>
                        <fk-reference-column>PRODUCT_ID</fk-reference-column>
                    </fk-column>
                </fk-columns>
            </foreign-key>
        </foreign-keys>
    </table>
    <table name="UserAddress">
        <columns>
            <column name="ADDRESS_ID">
                <data-type>INTEGER</data-type>
                <uniquevalue-generation>
                    <generator-name>UserAddress.ADDRESS_ID</generator-name>
                </uniquevalue-generation>
            </column>
            <column name="USER_ID">
                <data-type>INTEGER</data-type>
                <nullable>false</nullable>
            </column>
            <column name="ADDRESS">
                <data-type>CHAR</data-type>
                <nullable>false</nullable>
            </column>
        </columns>
        <primary-key name="ADDRESS_PK">
            <primary-key-column>ADDRESS_ID</primary-key-column>
        </primary-key>
        <foreign-keys>
            <foreign-key name="ADDRESS_FK" reference-table-name="CartUser">
                <fk-columns>
                    <fk-column>
                        <fk-local-column>USER_ID</fk-local-column>
                        <fk-reference-column>USER_ID</fk-reference-column>
                    </fk-column>
                </fk-columns>
            </foreign-key>
        </foreign-keys>
    </table>
    <table name="UserOrder">
        <columns>
            <column name="ORDER_ID">
                <data-type>INTEGER</data-type>
                <uniquevalue-generation>
                    <generator-name>ProductOrder.ORDER_ID</generator-name>
                </uniquevalue-generation>
            </column>
            <column name="USER_ID">
                <data-type>INTEGER</data-type>
                <nullable>false</nullable>
            </column>
            <column name="TIME_STAMP">
                <data-type>DATETIME</data-type>
                <nullable>false</nullable>
            </column>
            <column name="ADDRESS_ID">
                <data-type>INTEGER</data-type>
                <nullable>false</nullable>
            </column>
        </columns>
        <primary-key name="USER_ORDER_PK">
            <primary-key-column>ORDER_ID</primary-key-column>
        </primary-key>
        <foreign-keys>
            <foreign-key name="USER_ORDER_FK1" reference-table-name="CartUser">
                <fk-columns>
                    <fk-column>
                        <fk-local-column>USER_ID</fk-local-column>
                        <fk-reference-column>USER_ID</fk-reference-column>
                    </fk-column>
                </fk-columns>
            </foreign-key>
            <foreign-key name="USER_ORDER_FK2" reference-table-name="UserAddress">
                <fk-columns>
                    <fk-column>
                        <fk-local-column>ADDRESS_ID</fk-local-column>
                        <fk-reference-column>ADDRESS_ID</fk-reference-column>
                    </fk-column>
                </fk-columns>
            </foreign-key>
        </foreign-keys>
    </table>
    <table name="ProductOrder">
        <columns>
            <column name="ORDER_ID">
                <data-type>INTEGER</data-type>
                <nullable>false</nullable>
            </column>
            <column name="PRODUCT_ID">
                <data-type>INTEGER</data-type>
                <nullable>false</nullable>
            </column>
            <column name="PRODUCT_QUANTITY">
                <data-type>INTEGER</data-type>
                <nullable>false</nullable>
            </column>
            <column name="IS_CANCELLED">
                <data-type>BOOLEAN</data-type>
                <default-value>false</default-value>
            </column>
        </columns>
        <primary-key name="PRODUCT_ORDER_PK">
            <primary-key-column>ORDER_ID</primary-key-column>
            <primary-key-column>PRODUCT_ID</primary-key-column>
        </primary-key>
        <foreign-keys>
            <foreign-key name="ORDER_FK1" reference-table-name="UserOrder">
                <fk-columns>
                    <fk-column>
                        <fk-local-column>ORDER_ID</fk-local-column>
                        <fk-reference-column>ORDER_ID</fk-reference-column>
                    </fk-column>
                </fk-columns>
            </foreign-key>
            <foreign-key name="ORDER_FK2" reference-table-name="Product">
                <fk-columns>
                    <fk-column>
                        <fk-local-column>PRODUCT_ID</fk-local-column>
                        <fk-reference-column>PRODUCT_ID</fk-reference-column>
                    </fk-column>
                </fk-columns>
            </foreign-key>
        </foreign-keys>
    </table>
</data-dictionary>
